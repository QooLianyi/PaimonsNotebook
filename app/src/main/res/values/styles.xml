<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="paimonNoteTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/wave_ocean_blue_01</item>
        <item name="colorPrimaryDark">@color/menu_background_color</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="homeTopMenuTextStyle">
        <item name="android:textColor">@color/menu_un_select_color</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">5dp</item>
        <item name="android:layout_marginEnd">5dp</item>
    </style>

    <style name="CheckBoxStyle" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check_box_select</item>
    </style>


</resources>